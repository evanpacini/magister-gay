{
  "name": "Magister Gay Editionâ„¢",
  "description": "Turns your boring blue Magister into a vivid gay Magister.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "gaylistener.js"
  },
  "options_page": "gay.html",
  "content_scripts": [
    {
      "matches": ["https://*.magister.net/*"],
      "js": ["makeitgay.js"],
